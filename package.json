{
  "name": "angular-webpack-visualstudio",
  "version": "2.0.4",
  "description": "An Angular VS template",
  "main": "wwwroot/index.html",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"webpack-dev-server --https --env=dev --hot --inline\"",
    "webpack-dev": "webpack --env=dev",
    "webpack-production": "webpack --env=prod",
    "build-dev": "npm run webpack-dev",
    "build-production": "npm run webpack-production",
    "watch-webpack-dev": "webpack --env=dev --watch --color",
    "watch-webpack-production": "npm run build-production --watch --color",
    "publish-for-iis": "npm run build-production && dotnet publish -c Release",
    "test": "karma start",
    "test-watch": "karma start --no-single-run",
    "lint": "tslint ./angularApp"
  },
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/cli": "^6.0.8",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/platform-server": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@angular/upgrade": "6.0.9",
    "ajv": "^6.4.0",
    "angular-in-memory-web-api": "^0.6.0",
    "angular-popper": "^1.0.0-beta-1",
    "bootstrap": "^4.1.3",
    "codelyzer": "^4.4.2",
    "core-js": "2.5.1",
    "http-status-codes": "^1.3.0",
    "ie-shim": "0.1.0",
    "json2typescript": "^1.0.5",
    "popper.js": "^1.14.3",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.7.1",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.28",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.7.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.9.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.3",
    "typescript": "2.7.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.7.1"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch-webpack-dev"
    ]
  }
