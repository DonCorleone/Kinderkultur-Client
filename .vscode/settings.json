{
    "git.enableSmartCommit": true,
    "window.nativeTabs": false,
    "explorer.confirmDelete": false,
    "sftp.printDebugLog": true,
    "window.zoomLevel": 0,
    "editor.tabSize": 3,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.ngfactory.js": true,
        "**/*.ngstyle.js": true
    },
    "cSpell.words": [
        "imagename",
        "urldesc"
    ]
}